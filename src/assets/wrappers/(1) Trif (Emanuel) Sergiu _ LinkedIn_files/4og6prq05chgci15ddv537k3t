return Ember.RSVP.hash({postedJobsCount:e.queryURL(r,o("SEARCH_WORKFLOW_TRACKER_JOB_POSTING",{jobState:["OPEN","DRAFT","REVIEW","CLOSED"]})).catch((e=>a(e,t))).then((e=>Ember.get(e,"metadata.totalResultCount"))),myJobsCount:e.queryURL(r,o("SEARCH_MY_ITEMS_JOB_SEEKER",{cardType:l})).catch((e=>a(e,t))).then((e=>Ember.get(e,"metadata.totalResultCount"))),myLearningCount:e.queryURL(r,o("SEARCH_MY_ITEMS_LEARNING",{learningContentState:["SAVED","IN_PROGRESS","HISTORY"]})).catch((e=>a(e,t))).then((e=>Ember.get(e,"metadata.totalResultCount"))),savedPostsCount:e.queryURL(r,o("SEARCH_MY_ITEMS_SAVED_POSTS",{savedPostType:["ALL"]})).catch((e=>a(e,t))).then((e=>Ember.get(e,"metadata.totalResultCount"))),myProjectsCount:e.queryURL(r,o("SEARCH_MY_ITEMS_MARKETPLACES_PROJECTS",{projectState:["OPEN","CLOSED"]})).catch((e=>a(e,t))).then((e=>Ember.get(e,"metadata.totalResultCount")))}).then(s)}
const r="search/dash/clusters"
